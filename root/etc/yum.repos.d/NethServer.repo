#
# NethServer yum repositories configuration
#

[nethesis]
name=Nethesis development packages
baseurl=http://birro.nethesis.it/install/nethserver/NSA/nethesis
enabled=1

[centos63a]
name=CentOS 6.3 disk1
baseurl=http://birro.nethesis.it/install/nethserver/CentOS/6/repo1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-6
enabled=1

[centos63b]
name=CentOS 6.3 disk2
baseurl=http://birro.nethesis.it/install/nethserver/CentOS/6/repo2
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-6
enabled=1

[centos63c]
name=CentOS 6.3 updates
baseurl=http://birro.nethesis.it/install/nethserver/CentOS/6/updates
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-6
enabled=1

[nsa1]
name=NethServer NSA1 
baseurl=http://birro.nethesis.it/install/nethserver/NSA/nsa1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-NethServer-6
enabled=1

[nsa1.updates]
name=NethServer NSA1 updates
baseurl=http://birro.nethesis.it/install/nethserver/NSA/nsa1.updates
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-NethServer-6
